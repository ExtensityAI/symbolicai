#!/usr/bin/env python

import argparse
from symai.chat import SymbiaChat
from symai.components import Log


def parse_args() -> dict:
    parser = argparse.ArgumentParser(description='Process model parameters.')
    args = parser.parse_args()
    return args


def run(args: dict) -> None:
    chat = SymbiaChat()
    chat()


if __name__ == '__main__':
    args = parse_args()
    run(args)
